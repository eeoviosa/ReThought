{
  "name": "rethought",
  "version": "1.0.0",
  "description": "A public note board where anyone can share their thoughts. Built with React, Node.js, and MongoDB.",
  "main": "index.js",
  "scripts": {
    "build": "npm install --prefix backend && npm install --prefix frontend && npm run build --prefix frontend",
    "start": "npm run start --prefix backend",
    "dev": "concurrently \"npm run dev --prefix backend\" \"npm run dev --prefix frontend\"",
    "dev:backend": "npm run dev --prefix backend",
    "dev:frontend": "npm run dev --prefix frontend"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eeoviosa/ReThought.git"
  },
  "keywords": [
    "note-board",
    "chat-board",
    "nodejs",
    "react",
    "mongodb",
    "express",
    "tailwind-css",
    "full-stack",
    "open-source",
    "vite",
    "daisyui",
    "rate-limiting"
  ],
  "author": "",
